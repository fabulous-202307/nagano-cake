<div class="container">
  <h3>ジャンル編集</h3>
  <div class="row">
    <%= form_with model: [:admin, @genre] do |f| %>
      <div class="form-group">
        <%= f.label "ジャンル名" %>
        <%= f.text_field :name%>
        <%= f.submit "変更を保存", class: "btn btn-success rounded-pill" %>
      </div>
    <% end %>
  </div>
</div>
  